@*Booking*@
<section class="section-booking bg1-pattern">
    <div class="section-child-bg1">
        <div class="container  p-t-100 p-b-110">
            <div class="row">
                <div class="col-lg-12 p-b-30">
                    <div class="t-center">
                        <span class="tit2 t-center">
                            Liên Hệ
                        </span>

                        <h3 class="tit3 t-center m-b-35 m-t-15">
                            Đặt bàn
                        </h3>
                    </div>

                    <div class="wrap-form-booking">
                        <div class="row">
                            <div class="col-md-6">
                                <span class="txt9">
                                    Họ và Tên <span class="text-danger">*</span>
                                </span>

                                <div class="wrap-inputname size12 bo-rad-16 m-t-3 m-b-23">
                                    <input class="bo-rad-16 sizefull txt10 p-l-20" type="text" name="name" placeholder="Nguyễn Văn A...">
                                </div>

                                <span class="txt9">
                                    Ngày tổ chức <span class="text-danger">*</span>
                                </span>

                                <div class="wrap-inputdate pos-relative txt10 size12 bo-rad-16 m-t-3 m-b-23">
                                    <input class="my-calendar bo-rad-16 sizefull txt10 p-l-20" type="text" name="date">
                                    <i class="btn-calendar fa fa-calendar ab-r-m hov-pointer m-r-18" aria-hidden="true"></i>
                                </div>

                                <span class="txt9">
                                    Số người <span class="text-danger">*</span>
                                </span>

                                <div class="wrap-inputpeople size12 bo-rad-16 m-t-3 m-b-23">
                                    <input class="bo-rad-16 sizefull txt10 p-l-20" type="text" name="people" placeholder="4 người...">
                                </div>
                            </div>

                            <div class="col-md-6">


                                @*Phone*@
                                <span class="txt9">
                                    Số điện thoại <span class="text-danger">*</span>
                                </span>

                                <div class="wrap-inputphone size12 bo-rad-16 m-t-3 m-b-23">
                                    <input class="bo-rad-16 sizefull txt10 p-l-20" type="text" name="phone" placeholder="Phone">
                                </div>

                                <span class="txt9">
                                    Giờ tổ chức <span class="text-danger">*</span>
                                </span>

                                <div class="wrap-inputtime size12 bo-rad-16 m-t-3 m-b-23">
                                    <select class="selection-1" name="time">
                                        <option>07:00</option>
                                        <option>07:30</option>
                                        <option>08:00</option>
                                        <option>09:30</option>
                                        <option>09:00</option>
                                        <option>09:30</option>
                                        <option>10:00</option>
                                        <option>10:30</option>
                                        <option>11:00</option>
                                        <option>11:30</option>
                                        <option>12:00</option>
                                        <option>12:30</option>
                                        <option>13:00</option>
                                        <option>13:30</option>
                                        <option>14:00</option>
                                        <option>14:30</option>
                                        <option>15:00</option>
                                        <option>15:30</option>
                                        <option>16:00</option>
                                        <option>16:30</option>
                                        <option>17:00</option>
                                        <option>17:30</option>
                                        <option>18:00</option>
                                        <option>18:30</option>
                                        <option>19:00</option>
                                        <option>19:30</option>
                                        <option>20:00</option>
                                        <option>20:30</option>
                                        <option>21:00</option>
                                        <option>21:30</option>
                                        <option>22:00</option>
                                    </select>
                                </div>

                                @*Email*@
                                <span class="txt9">
                                    Địa chỉ Email <span class="text-danger">*</span>
                                </span>

                                <div class="wrap-inputemail size12 bo-rad-16 m-t-3 m-b-23">
                                    <input class="bo-rad-16 sizefull txt10 p-l-20" type="text" name="email" placeholder="emailcuaban@email.com">
                                </div>

                            </div>
                            <div class="col-md-12">
                                <span class="txt9">
                                    Ghi chú
                                </span>
                                <textarea class="bo-rad-16 size35 txt10 p-l-20 p-t-15 m-b-10 m-t-3" rows="4" name="ghichu" placeholder="Ghi chú thêm (nếu có)"></textarea>

                            </div>
                        </div>

                        <div class="wrap-btn-booking flex-c-m m-t-6">
                            @*Button3*@
                            <button type="submit" class="btn1 flex-c-m size1 txt3 trans-0-4">
                                gửi thông tin
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>

